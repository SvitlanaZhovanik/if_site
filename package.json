    "@react-email/render": "^0.0.11",
    "autoprefixer": "10.4.15",
    "clsx": "^2.0.0",
    "cookies-next": "^4.1.0",
    "next": "^14.0.3",
    "nodemailer": "^6.9.8",
    "postcss": "8.4.29",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flags-select": "^2.2.3",
