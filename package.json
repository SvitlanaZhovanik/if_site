{
  "name": "company_site",
  "description": "Next.js boilerplate with TypeScript, Tailwind CSS, ESLint, Prettier, Husky, and more.",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/josserden/nextjs-boilerplate"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "tailwindcss",
    "eslint",
    "prettier",
    "husky",
    "boilerplate"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
